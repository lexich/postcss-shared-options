{
  "name": "postcss-shared-options",
  "version": "0.0.0",
  "description": "PostCSS plugin Share variables between different css files with scope",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "variables"
  ],
  "author": "Efremov Alexey <lexich121@gmail.com>",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "lexich/postcss-shared-options",
  "bugs": {
    "url": "https://github.com/lexich/postcss-shared-options/issues"
  },
  "homepage": "https://github.com/lexich/postcss-shared-options",
  "dependencies": {
    "lodash": "^4.13.1",
    "postcss": "^5.1.0",
    "postcss-value-parser": "^3.3.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "eslint-config-postcss": "^2.0.2",
    "eslint-plugin-flowtype": "^2.3.1",
    "flow-bin": "^0.29.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run ava",
    "ava": "ava ./tests",
    "eslint": "eslint src/*.js",
    "compile": "node_modules/.bin/babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "espower",
      "transform-runtime"
    ]
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
